{
  "builds": [
    {
      "src": "vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon(.*).ico",
      "dest": "/static/favicon$1.ico"
    },
    {
      "src": "/(.*).png",
      "dest": "/static/$1.png"
    },
    {
      "src": "/(.*).xml",
      "dest": "/static/$1.xml"
    },
    {
      "src": "/(.*).webmanifest",
      "dest": "/static/$1.webmanifest"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel.py"
    }
  ]
}